{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "paths": {
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@screens/*": [
        "./src/screens/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@tab/*": [
        "./src/tabs/*"
      ],
      "@const/*": [
        "./src/constants/*"
      ],
      "@/*": [
        "./src/*"
      ],
    }
  }
}